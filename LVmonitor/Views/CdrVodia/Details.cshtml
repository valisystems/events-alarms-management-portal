@model LvMonitor.Models.Lvm_CdrVodia

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal">&times;</button>
    <h3 class="modal-title">Call Details</h3>
</div>
<div class="modal-body">
    <dl class="dl-horizontal lvm-dl-horizontal">
        <dt>
            CDR Type
        </dt>
        <dd>
            Vodia
        </dd>
        <dt>
            Facility
        </dt>
        <dd>
            @Html.DisplayFor(model => model.FacilityName)
        </dd>
        <dt>
            Action
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Action)
        </dd>
        <dt>
            System
        </dt>

        <dd>
            @Html.DisplayFor(model => model.System)
        </dd>

        <dt>
            Primary Call ID
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PrimaryCallID)
        </dd>

        <dt>
            Call ID
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CallID)
        </dd>

        <dt>
            From
        </dt>

        <dd>
            @Html.DisplayFor(model => model.From)
        </dd>

        <dt>
            To
        </dt>

        <dd>
            @Html.DisplayFor(model => model.To)
        </dd>

        <dt>
            Direction
        </dt>

        <dd>
            @if (Model.Direction == true)
            { <text>Inbound</text> }
            else
            { <text>Outbound</text> }
        </dd>

        <dt>
            Remote Party
        </dt>

        <dd>
            @Html.DisplayFor(model => model.RemoteParty)
        </dd>

