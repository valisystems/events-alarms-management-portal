        <dt>
            Trunk ID
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TrunkID)
        </dd>

        <dt>
            Cost
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Cost)
        </dd>

        <dt>
            CMC
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CMC)
        </dd>

        <dt>
            Domain
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Domain)
        </dd>

        <dt>
            Time Start
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TimeStart)
